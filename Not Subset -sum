

class Solution {
    public long findSmallest(int[] arr) {
        // Your code goes here
        long ans=1;
        int n=arr.length;
        for(int i=0;i<n;i++){
            if(ans<arr[i]){
                return ans;
                
            }
            else{
                ans+=arr[i];
                
            }
        }
        return ans;
    }
}
