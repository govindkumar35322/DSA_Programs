

//User function Template for Java

class Solution {
    int print2largest(int arr[], int n) {
        Arrays.sort(arr);
        if(n<2){
            return -1;
        }
        for(int i = n-2;i>=0;i--){
            if(arr[i]<arr[n-1]){
                return arr[i];
            }
        }
         return -1;
        
    }
}
