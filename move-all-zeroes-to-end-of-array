class Solution {
    void pushZerosToEnd(int[] arr) {
        int n=arr.length;
        int start=0;
      for(int i=0;i<n;i++){
          if(arr[i]!=0){
          arr[start++]=arr[i];
         }
       
      }
          while(start<n){
              arr[start++]=0;
               
          }
      
      
       
    }
}
